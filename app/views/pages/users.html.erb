
  
  
  



<table class="table">
  <thead>
    <tr>
      
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">E-mail</th>
      <th scope="col">User ID</th>
      <th scope="col">Is it admin?</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @users.each do |user| %>
      <td><%= user.first_name %></td>
      
      <td><%= user.last_name %></td>
      
      <td><%= user.email %></td>
      <td><%= user.id %> </td>
      <td><% if user.admin  %>
            Admin: Yes
            <% else %>
            Admin: no 
          <% end %> </td>
    </tr>
    <% end %>
  </tbody>
</table>